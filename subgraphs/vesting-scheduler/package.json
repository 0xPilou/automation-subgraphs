{
  "name": "vesting-scheduler-subgraph",
  "version": "1.0.0",
  "description": "A subgraph for the vesting scheduler contracts",
  "keywords": [
    "subgraph"
  ],
  "license": "MIT",
  "author": "Superfluid",
  "scripts": {
    "build": "graph build ./eth-mainnet.subgraph.yaml",
    "deploy:test": "graph deploy --product hosted-service tokdaniel/vesting-v1-mumbai-test polygon-mumbai.subgraph.yaml",
