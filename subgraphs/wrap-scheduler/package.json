{
  "name": "wrap-scheduler-subgraph",
  "version": "1.0.0",
  "description": "A subgraph for the auto-wrap scheduler contracts",
  "keywords": [
    "subgraph"
  ],
  "license": "MIT",
  "author": "Superfluid",
  "scripts": {
    "build": "graph build ./eth-mainnet.subgraph.yaml",
    "deploy:test": "graph deploy --product hosted-service msoni89/auto-wrap-v1-mumbai-test polygon-mumbai.subgraph.yaml",
    "dev": "nodemon -e ts -x yarn test",
    "gen": "npm-run-all -s gen:yaml gen:types",
    "gen:types": "graph codegen ./eth-mainnet.subgraph.yaml --output-dir src/types/",
    "gen:yaml": "ts-node ./scripts/generate-yamls.ts",
    "test": "graph test"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.55.0",
    "@graphprotocol/graph-ts": "0.31.0",
    "@types/node": "^20.5.4",
    "assemblyscript-json": "1.1.0",
    "matchstick-as": "0.5.2",
    "mustache": "4.2.0",
    "nodemon": "3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
