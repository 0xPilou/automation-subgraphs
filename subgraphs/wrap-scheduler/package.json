    "assemblyscript": "^0.19.0",
    "assemblyscript-json": "^1.1.0",
    "matchstick-as": "^0.5.2",
    "mustache": "^4.2.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
