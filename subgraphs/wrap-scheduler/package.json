  ],
  "license": "MIT",
  "author": "Superfluid",
  "scripts": {
    "build": "graph build ./eth-mainnet.subgraph.yaml",
    "deploy:test": "graph deploy --product hosted-service msoni89/auto-wrap-v1-mumbai-test polygon-mumbai.subgraph.yaml",
